    "help": {
      "title": "Help",
      "children": {
        "guide": {
          "title": "Guide",
          "children": {
            "manipulation": {
              "title": "Manipulating Objects",
              "children": {
                "add": {
                  "title": "Add",
                  "content": "Select a shape type (ex: circle, arrow, rectangle) from the top toolbar, then click or drag on the screen to create an object. With the free drawing tool, you can draw directly by clicking and moving the mouse."
                },
                "edit": {
                  "title": "Edit",
                  "content": "Click on an object to select it for editing. You can drag, resize with the mouse, or change its properties displayed in the right-hand toolbar."
                },
                "group": {
                  "title": "Group",
                  "content": "Hold down the Ctrl key and click to select multiple objects you want to group. Then click the group button on the toolbar."
                }
              }
            },
            "save": {
              "title": "Save and Open",
              "content": "Use the Import/Export function to save drawings in various formats or reopen saved drawings from your device or the cloud."
            },
            "template": {
              "title": "Template",
              "content": "You can save one or multiple objects as a template/component for reuse in other drawings."
            },
            "shotcut": {
              "title": "Shortcut",
              "children": {
                "general": {
                  "title": "General",
                  "content": [
                    "Ctrl + Click/Cmd + Click: Select multiple objects.",
                    "Ctrl + A/Cmd + A: Select all objects.",
                    "Del/Delete: Delete object.",
                    "Ctrl + X/Cmd + X: Cut object.",
                    "Ctrl + C/Cmd + C: Copy object.",
                    "Ctrl + V/Cmd + V: Paste object.",
                    "Ctrl + D/Cmd + D: Duplicate object.",
                    "Ctrl + S/Cmd + S: Save.",
                    "Ctrl + Shift + S/Cmd + Shift + S: Save to cloud.",
                    "Ctrl + Plus/Cmd + Plus: Zoom in.",
                    "Ctrl + Minus/Cmd + Minus: Zoom out.",
                    "Ctrl + 0/Cmd + 0: Fit to screen.",
                    "Esc/Escape: End free drawing/Deselect object.",
                    "Ctrl + W/Cmd + W: Close page."
                  ]
                },
                "history": {
                  "title": "History",
                  "content": [
                    "Ctrl + Z/Cmd + Z: Undo.",
                    "Ctrl + Y/Cmd + Y: Redo."
                  ]
                },
                "text": {
                  "title": "Text",
                  "content": [
                    "Ctrl + B/Cmd + B: Bold.",
                    "Ctrl + I/Cmd + I: Italic.",
                    "Ctrl + U/Cmd + U: Underline.",
                    "Ctrl + T/Cmd + T: Strikethrough.",
                    "Ctrl + L/Cmd + L: Align left.",
                    "Ctrl + E/Cmd + E: Align center.",
                    "Ctrl + R/Cmd + R: Align right.",
                    "Ctrl + Shift + L/Cmd + Shift + L: Align top.",
                    "Ctrl + Shift + E/Cmd + Shift + E: Align middle.",
                    "Ctrl + Shift + R/Cmd + Shift + R: Align bottom."
                  ]
                }
              }
            }
          }
        },
        "about": {
          "title": "About",
          "children": {
            "introdution": {
              "title": "Introduction",
              "content": "This is an intuitive tool that helps users create, edit, and manage content quickly and efficiently. With a user-friendly interface, the tool allows direct interaction on the screen, making it easy to draw basic shapes, perform freehand drawing, insert images, add text, use icons, or insert videos."
            },
            "version": {
              "title": "Version",
              "content": "Current version: 1.0.0."
            },
            "feature": {
              "title": "Features",
              "children": {
                "object": {
                  "title": "Supported Objects",
                  "children": {
                    "basic": {
                      "title": "Basic Shapes",
                      "content": "Line, arrow, circle, rectangle, convex polygon, concave polygon, ring, sector, quadratic and bezier curve."
                    },
                    "text": {
                      "title": "Text",
                      "content": "Add text blocks for descriptions, annotations, or titles within the drawing."
                    },
                    "image": {
                      "title": "Image",
                      "content": "Insert images from your device or via links to illustrate or enhance visual content."
                    },
                    "icon": {
                      "title": "Icons",
                      "content": "A library with over 3000 diverse icons that can be used to highlight or complement your drawings."
                    },
                    "video": {
                      "title": "Video",
                      "content": "Insert videos from your device or via links to enhance visualization and interactivity."
                    },
                    "multiLine": {
                      "title": "Multi Line",
                      "content": "Allows drawing multi lines on the screen."
                    },
                    "freeDrawing": {
                      "title": "Free Drawing",
                      "content": "Allows drawing freely on the screen, suitable for sketching or quick creative work."
                    }
                  }
                },
                "manipulation": {
                  "title": "Manipulation With Objects",
                  "children": {
                    "add": {
                      "title": "Add",
                      "content": "Add new objects by clicking or dragging and dropping them directly into the workspace."
                    },
                    "edit": {
                      "title": "Edit",
                      "content": "Supports editing actions: dragging, resizing one or multiple objects at once, and modifying object details."
                    },
                    "group": {
                      "title": "Group",
                      "content": "Supports grouping objects."
                    }
                  }
                },
                "template": {
                  "title": "Save And Reuse",
                  "content": "Allows saving objects or drawings as templates/components for reuse."
                },
                "io": {
                  "title": "Import/Export",
                  "content": "Supports importing/exporting data from/to devices and cloud in multiple formats for sharing or storage."
                },
                "shotcut": {
                  "title": "Shortcuts",
                  "content": "A system of shortcuts that enables faster operations, saving time for professional users."
                },
                "history": {
                  "title": "History",
                  "content": "Automatically saves operation history, allowing you to undo or redo when necessary."
                },
                "other": {
                  "title": "Other Features",
                  "content": "Changing background color, displaying alignment guides, toggling grid, and many other useful features."
                }
              }
            },
            "comingSoonFeature": {
              "title": "Coming Soon Features",
              "children": {
                "improve": {
                  "title": "Performance",
                  "content": "Improving for better performance."
                }
              }
            },
            "license": {
              "title": "License",
              "content": "This product is developed and copyrighted by Viettel HighTech."
            }
          }
        }
